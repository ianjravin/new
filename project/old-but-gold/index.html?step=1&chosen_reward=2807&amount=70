    background: -webkit-linear-gradient(bottom, %bottomgrad% 0%, %color% 100%); /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(to top, %bottomgrad% 0%, %color% 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='%bottomgrad%', endColorstr='%color%',GradientType=1 ); /* IE6-9 */
}
