.progress .bar{
    /* Permalink - use to edit and share this gradient: http://colorzilla.com/gradient-editor/#1e5799+0,82f3bd+0,7db9e8+100 */
    background: %topgrad%; /* Old browsers */
    background: -moz-linear-gradient(left, %bottomgrad%  0%, %color% 100%); /* FF3.6-15 */
    background: -webkit-linear-gradient(left, %bottomgrad% 0%, %color% 100%); /* Chrome10-25,Safari5.1-6 */
    background: linear-gradient(to right, %bottomgrad% 0%, %color% 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='%bottomgrad%', endColorstr='%color%',GradientType=1 ); /* IE6-9 */
}
